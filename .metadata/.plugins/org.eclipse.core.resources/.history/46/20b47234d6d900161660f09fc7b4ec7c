import java.util.regex.*;
import java.io.*;

public class ReadFile {
public static void main(String[] args)
{
	try(FileInputStream is = new FileInputStream("/users/downloads/shapes.xml")){
		InputStreamReader ir = new InputStreamReader(is);
		BufferedReader rdr = new BufferedReader(ir);
		String line = rdr.readLine();
		while(line!= null);{
			Pattern shapes = Pattern.compile(("\"(.*?)\""));
			Matcher matcher = shapes.matcher(line);
			if(matcher.find())
				System.out.printf("Shape: %s\n", matcher.group(1));
			line = rdr.readLine();
		}
	}
	catch (Exception ex){System.out.printf("Failed for %s\n", "shapes.xml");}
}
}
