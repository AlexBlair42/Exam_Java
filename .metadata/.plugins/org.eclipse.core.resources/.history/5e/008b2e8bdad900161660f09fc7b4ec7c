import java.awt.*;
import javax.swing.*;

public class UI extends JComponent {
	JFrame frame; 
	
	public UI(){
	frame = new JFrame();
	frame.setBounds(300,300,300,300);
	frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	frame.setVisible(true);
	}
	
	public static void main(String[] args)
	{
		UI u = new UI();
	}
}
